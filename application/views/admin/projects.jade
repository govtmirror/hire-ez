- Section::inject('no_page_header', true)

!{View::make('admin.partials.subnav')->with('current_page', 'projects')}

table.table.table-bordered.table-striped.admin-projects-table
  thead
    tr
      th id
      th title
      th fork_count
      th recommended
      th public
        !{Helper::helper_tooltip(__("r.admin.projects.project_helper_text"), "top")}
      th project_type

  tbody#projects-tbody
    :javascript
      | $(function(){
      |  new Rfpez.Backbone.AdminProjects( !{$projects_json} )
      | })

.pagination-wrapper
  !{$projects->links()}